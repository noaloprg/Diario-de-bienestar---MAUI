<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Diario_bienestar.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Diario_bienestar"
    Title="Diario_bienestar"
    FlyoutBackground="{StaticResource RosaPalo}"
    FlyoutWidth="200">

    <ShellContent
        Title="BIENVENIDO"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Registro"
        ContentTemplate="{DataTemplate local:RegistroDiario}"
        Route="Registro" />

    
</Shell>
